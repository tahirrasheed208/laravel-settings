{
    "name": "tahirrasheed208/laravel-settings",
    "description": "Store your application settings",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Tahir Rasheed",
            "email": "tahirrasheed208@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^11.0|^9.5"
    },
    "autoload": {
        "psr-4": {
            "TahirRasheed\\LaravelSettings\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TahirRasheed\\LaravelSettings\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TahirRasheed\\LaravelSettings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
